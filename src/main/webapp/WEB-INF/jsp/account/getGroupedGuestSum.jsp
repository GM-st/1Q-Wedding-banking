<script>


	let groupedGuestSumNameList = [];

	let groupedGuestSumValueList = [];
		
		fetch("member/getGroupedGuestSum")
		.then(res=>res.json())
		.then(res=>{
			let transactionList = res;
			for(let i=0 ;i<transactionList.length;i++){
				
				groupedGuestSumNameList.push(transactionList[i].GUEST_GROUP);
				groupedGuestSumValueList.push(transactionList[i].COUNT);
			
			}
			
		})
		.then(res=>{
			
			Highcharts.chart('container3', {
			    chart: {
			        type: 'column',
			        options3d: {
			            enabled: true,
			            alpha: 10,
			            beta: 25,
			            depth: 70
			        }
			    },
			    title: {
			        text: 'External trade in goods by country, Norway 2021'
			    },
			    subtitle: {
			        text: 'Source: ' +
			            '<a href="https://www.ssb.no/en/statbank/table/08804/"' +
			            'target="_blank">SSB</a>'
			    },
			    plotOptions: {
			        column: {
			            depth: 25
			        }
			    },
			    xAxis: {
			        categories: groupedGuestSumNameList,
			        labels: {
			            skew3d: true,
			            style: {
			                fontSize: '16px'
			            }
			        }
			    },
			    yAxis: {
			        title: {
			            text: 'NOK (million)',
			            margin: 20
			        }
			    },
			    tooltip: {
			        valueSuffix: ' MNOK'
			    },
			    series: [{
			        name: 'Total imports',
			        data: groupedGuestSumValueList
			    }]
			});

		})
		



</script>
	
		
	






